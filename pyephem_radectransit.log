# define stuff
import ephem
ata = ephem.Observer()
ata.lat = '40.817'
ata.long = '-121.47'
ata.elevation = 3000  # guess
ata.date = ephem.now()

# current 
print ata.sidereal_time()

# set up source
src = ephem.FixedBody()
src._ra = '17:45:00'
src._dec = '-29:00:00'
src.compute(ata)

# get transit properties -- note times are normally in UT!
print ephem.localtime(ata.next_rising(src))
print ephem.localtime(ata.next_setting(src))
print ephem.localtime(ata.transit(src))
print src.transit_alt
